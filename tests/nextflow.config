/*
========================================================================================
    Nextflow config file for running tests
========================================================================================
*/

process {
   withName: DOWNLOAD_NCBI { 
     errorStrategy = 'retry'
     maxRetries    = 2
   }
   withName: GFFREAD {
     container = 'ecoflowucl/gffread_python:python-3.10_Linux_x86_64'
   }
}

timeline.enabled = false

report.enabled = false

dag.enabled = false
